{
  "implicitDependencies": {
    "workspace.json": "*",
    "package.json": { "dependencies": "*", "devDependencies": "*" },
    "tsconfig.base.json": "*",
    "tslint.json": "*",
    ".eslintrc.json": "*",
    "nx.json": "*"
  },
  "affected": { "defaultBase": "master" },
  "npmScope": "microservices-realworld-example-app",
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"],
        "accessToken": "ODgyMDQyYmQtMWEzOS00OGYwLWExYjEtOTUxM2JmYzQ1YmE4fHJlYWQtd3JpdGU="
      }
    }
  },
  "targetDependencies": {
    "build": [{ "target": "build", "projects": "dependencies" }]
  },
  "cli": { "defaultCollection": "@nrwl/nest" },
  "generators": {
    "@nrwl/react": {
      "application": {
        "style": "styled-components",
        "linter": "eslint",
        "babel": true
      },
      "component": { "style": "styled-components" },
      "library": { "style": "styled-components", "linter": "eslint" }
    }
  },
  "defaultProject": "realworld-example-app"
}
